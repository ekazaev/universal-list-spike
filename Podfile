platform :ios, '11.0'

inhibit_all_warnings!

def shared_pods
  pod 'DifferenceKit', '1.1.5'
end

target 'UniversalListController' do
  shared_pods
end

target 'Shared' do
  shared_pods
end

target 'SharedUI' do
  shared_pods
end

target 'Search' do
  shared_pods
end

target 'RandomList' do
  shared_pods
end
